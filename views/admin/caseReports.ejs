<%- include('adminPartials/head')%>

<body class="inner_page general_elements">
<div class="full_container">
    <div class="inner_container">
        <!-- Sidebar  -->
        <%- include('adminPartials/sidebar')%>
        <!-- end sidebar -->
        <!-- right content -->
        <div id="content">
            <!-- topbar -->
            <%- include('adminPartials/adminBar')%>
            <!-- end topbar -->
            <!-- dashboard inner -->
            <div class="midde_cont">
                <div class="container-fluid">
                    <div class="row column_title">
                        <div class="col-md-12">
                            <div class="page_title">
                                <h2>Case Reports</h2>
                            </div>
                        </div>
                    </div>
                    <!-- row -->
                    <div class="row column4 graph">
                        <%report.forEach(function(report, num){%>
                            <div class="col-md-6">
                                <div class="white_shd full margin_bottom_30">
                                    <div class="full graph_head">
                                        <div class="heading1 margin_0">
                                            <h2>Case #<%=report.cnum%></h2>
                                        </div>
                                    </div>
                                    <div class="full inner_elements">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="tab_style2">
                                                    <div class="tabbar padding_infor_info">
                                                        <nav>
                                                            <div class="nav nav-tabs" id="nav-tab<%=num%>" role="tablist">
                                                                <a class="nav-item nav-link active" id="nav-home-tab<%=num%>" data-toggle="tab" href="#nav-home_s<%=num%>" role="tab" aria-controls="nav-home_s<%=num%>" aria-selected="true">Case Intro</a>
                                                                <a class="nav-item nav-link" id="nav-profile-tab<%=num%>" data-toggle="tab" href="#nav-profile_s<%=num%>" role="tab" aria-controls="nav-profile_s<%=num%>" aria-selected="false">Bio</a>
                                                                <a class="nav-item nav-link" id="nav-contact-tab<%=num%>" data-toggle="tab" href="#nav-contact_s<%=num%>" role="tab" aria-controls="nav-contacts_s<%=num%>" aria-selected="false">Contact</a>
                                                                <a class="nav-item nav-link" id="nav-status-tab<%=num%>" data-toggle="tab" href="#nav-status_s<%=num%>" role="tab" aria-controls="nav-status_s<%=num%>" aria-selected="false">Status</a>
                                                            </div>
                                                        </nav>
                                                        <div class="tab-content" id="nav-tabContent_<%=num%>">
                                                            <div class="tab-pane fade show active" id="nav-home_s<%=num%>" role="tabpanel" aria-labelledby="nav-home-tab">
                                                                <p><%=report.cintro%>
                                                                </p>
                                                            </div>
                                                            <div class="tab-pane fade" id="nav-profile_s<%=num%>" role="tabpanel" aria-labelledby="nav-profile-tab">
                                                                <p><%=report.cbio%>>
                                                                </p>
                                                            </div>
                                                            <div class="tab-pane fade" id="nav-contact_s<%=num%>" role="tabpanel" aria-labelledby="nav-contact-tab">
                                                                <span class="reportBio">Name:<%=report.cname%></span><br>
                                                                <span class="reportBio">Email:<%=report.cmail%></span><br>
                                                                <span class="reportBio">Phone:<%=report.cphone%></span><br>
                                                            </div>
                                                            <div class="tab-pane fade" id="nav-status_s<%=num%>" role="tabpanel" aria-labelledby="nav-status-tab">
                                                                <p contenteditable="true"><%=report.cStatus%>
                                                                </p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <%});%>

                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="add_report">
                                        <a href="/adminDash/addCaseReport"><button class="add_member add_report">NEW CASE REPORT</button></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end dashboard inner -->
                </div>
            </div>
            <!-- model popup -->
            <!-- The Modal -->
            <%- include('adminPartials/modalPop')%>
            <!-- end model popup -->
        </div>

<%- include('adminPartials/foot')%>

