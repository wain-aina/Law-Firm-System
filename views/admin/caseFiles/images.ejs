<%- include('../adminPartials/head')%>

<div class="flexbox-search">
    <div class="search">
        <form id="getInvoice" method="get" action="/adminDash/imagesearch">
            <input class='find' type="text" name='imageSearch' placeholder="Search . . ." autocomplete="off" required>
        </form>
    </div>

</div>
<script>
    document.getElementById("getInvoice").keyup = function(e) {
        var key = e.charCode || e.keyCode || 0;
        if (key === 13) {
            e.preventDefault();
        }
    }
</script>

<div class="row">
<%images.forEach(function(pic){%>
    <div class="col-xl-2 col-lg-2 col-md-3 col-sm-6">
        <div class="folder_icon">
            <a href="https://localhost:3000/<%=pic.path%>" target="_blank"><i class="fa fa-folder fa-5x red_color" aria-hidden="true"></i></a>
        </div>
        <div class="folder_name" ><%=pic.filename%></div>
    </div>
<%})%>
</div>
<div class="container" style="margin: 7% 36%">
    <form action="/adminDash/images" enctype="multipart/form-data" method="POST">

        <div class="row">
        <div class="col-sm-12 col-lg-3 box">
            <input type="file" name="myFile" id="file-5" class="inputfile inputfile-4" data-multiple-caption="{count} files selected" multiple />
            <label class="file-5" for="file-5"><figure><svg xmlns="http://www.w3.org/2000/svg" width="20" height="17" viewBox="0 0 20 17"><path d="M10 0l-5.2 4.9h3.3v5.1h3.8v-5.1h3.3l-5.2-4.9zm9.3 11.5l-3.2-2.1h-2l3.4 2.6h-3.5c-.1 0-.2.1-.2.1l-.8 2.3h-6l-.8-2.2c-.1-.1-.1-.2-.2-.2h-3.6l3.4-2.6h-2l-3.2 2.1c-.4.3-.7 1-.6 1.5l.6 3.1c.1.5.7.9 1.2.9h16.3c.6 0 1.1-.4 1.3-.9l.6-3.1c.1-.5-.2-1.2-.7-1.5z"/></svg></figure> <span class="wow">Choose a file&hellip;</span></label>
        </div>

        <div class="col-sm-12 col-lg-3 box">
            <input type="submit" id="submit-btn" class="inputfile inputfile-4" value="Upload File(s)">
            <label class="file-5" for="submit-btn"><figure><svg xmlns="http://www.w3.org/2000/svg" width="20" height="17" viewBox="0 0 20 17"><path d="M10 0l-5.2 4.9h3.3v5.1h3.8v-5.1h3.3l-5.2-4.9zm9.3 11.5l-3.2-2.1h-2l3.4 2.6h-3.5c-.1 0-.2.1-.2.1l-.8 2.3h-6l-.8-2.2c-.1-.1-.1-.2-.2-.2h-3.6l3.4-2.6h-2l-3.2 2.1c-.4.3-.7 1-.6 1.5l.6 3.1c.1.5.7.9 1.2.9h16.3c.6 0 1.1-.4 1.3-.9l.6-3.1c.1-.5-.2-1.2-.7-1.5z"/></svg></figure> <span>Upload file(s);</span></label>
        </div>
        </div>

    </form>
</div>
<script src="/js/uploadBtn.js"></script>

</body>
</html>