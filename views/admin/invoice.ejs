<%- include('adminPartials/head')%>

<body class="inner_page invoice_page">
<div class="full_container">
    <div class="inner_container">
        <!-- Sidebar  -->
        <%- include('adminPartials/sidebar')%>
        <!-- end sidebar -->
        <!-- right content -->
        <div id="content">
            <!-- topbar -->
            <%- include('adminPartials/adminBar')%>
            <!-- end topbar -->
            <!-- dashboard inner -->
            <div class="midde_cont">
                <div class="container-fluid">
                    <div class="row column_title">
                        <div class="col-md-12">
                            <div class="page_title"><%- include('../partials/alerts')%></div>
                        </div>
                    </div>
                    <!-- row -->

                    <div class="row" id="printable">
                        <!-- invoice section -->
                        <div class="col-md-12">
                            <div class="white_shd full margin_bottom_30">
                                <div class="full graph_head">
                                    <div class="heading1 margin_0">
                                        <h2><i class="fa fa-file-text-o"></i> Fee Note</h2>
                                        <div class="invoice-btns">
                                            <%if(invoice.length > 0){%>
                                                <button id="printpagebutton" class="btn bg-white btn-light mx-1px text-95 invoice-tools printButton printpagebutton" data-title="PDF" onclick="printpage()">
                                                    <i class="mr-1 fa fa-file-pdf-o text-danger-m1 text-120 w-2"></i>
                                                    Print PDF
                                                </button>
                                                <button id="exportpagebutton" class="btn bg-white btn-light mx-1px text-95 invoice-tools printButton printpagebutton" data-title="PDF" onclick="GeneratePdf();">
                                                    <i class="mr-1 fa fa-file-pdf-o text-danger-m1 text-120 w-2"></i>
                                                    Export PDF
                                                </button>
                                            <%}%>
                                        </div>
                                    </div>
                                </div>
                                <div class="full" id="section-to-print">
                                    <div class="invoice_inner">

                                        <div class="page-content container">
                                            <div class="page-header text-blue-d2">
                                            <div class="container px-0">
                                                <div class="row mt-4">
                                                    <div class="col-12 col-lg-12">
                                                        <div class="row">
                                                            <div class="col-12">
                                                                <div class="text-center text-150">
                                                                    <i class="fa fa-gavel fa-2x text-success-m2 mr-1"></i>
                                                                    <span class="text-default-d3">Sigani Associates</span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- .row -->
                                                        <%invoice.forEach(function(invoice, num){%>

                                                        <hr class="row brc-default-l1 mx-n1 mb-4" />

                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                <div>
                                                                    <span class="text-sm text-grey-m2 align-middle">To:</span>
                                                                    <span class="text-600 text-110 text-blue align-middle"><%=invoice.cname%></span>
                                                                </div>
                                                                <div class="text-grey-m2">
                                                                    <div class="my-1">
                                                                        Email Address: <%=invoice.cEmail%>
                                                                    </div>

                                                                    <div class="my-1"><i class="fa fa-phone fa-flip-horizontal text-secondary"></i> <b class="text-600"><%=invoice.cphone%></b></div>
                                                                </div>
                                                            </div>
                                                            <!-- /.col -->

                                                            <div class="text-95 col-sm-6 align-self-start d-sm-flex justify-content-center">
                                                                <hr class="d-sm-none" />
                                                                <div class="text-grey-m2">
                                                                    <div class="mt-1 mb-2 text-secondary-m1 text-600 text-125">
                                                                        Invoice
                                                                    </div>

                                                                    <div class="my-2"><i class="fa fa-circle text-blue-m2 text-xs mr-1"></i> <span class="text-600 text-90">ID:</span> #<%=invoice.cid%></div>

                                                                    <div class="my-2"><i class="fa fa-circle text-blue-m2 text-xs mr-1"></i> <span class="text-600 text-90">Issue Date:</span><%=invoice.cissue%></div>

                                                                </div>
                                                            </div>
                                                            <!-- /.col -->
                                                        </div>

                                                        <div class="mt-4">
                                                            <div class="row text-600 text-white bgc-default-tp1 py-25">
                                                                <div class="d-none d-sm-block col-1">#</div>
                                                                <div class="col-9 col-sm-5">Description</div>
                                                                <div class="d-none d-sm-block col-4 col-sm-2">Qty</div>
                                                                <div class="d-none d-sm-block col-sm-2">Unit Price</div>
                                                                <div class="col-2">Amount</div>
                                                            </div>

                                                                <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                    <div class="d-none d-sm-block col-1"><%=num+1%></div>
                                                                    <div class="col-9 col-sm-5"><%=invoice.serv1%></div>
                                                                    <div class="d-none d-sm-block col-2"><%=invoice.quan1%></div>
                                                                    <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit1%></div>
                                                                    <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit1 * invoice.quan1%></div>
                                                                </div>

                                                            <%if(invoice.serv2.length && invoice.quan2.length && invoice.unit2.length !== 0){%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+2%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv2%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan2%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit2%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit2 * invoice.quan2%></div>
                                                            </div>
                                                            <%}%>

                                                           <% if ( invoice.serv3.length && invoice.quan3.length && invoice.unit3.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+3%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv3%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan3%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit3%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit3 * invoice.quan3%></div>
                                                            </div>
                                                            <%} %>

                                                            <% if ( invoice.serv4.length && invoice.quan4.length && invoice.unit4.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+3%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv4%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan4%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit4%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit4 * invoice.quan4%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv5.length && invoice.quan5.length && invoice.unit5.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+4%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv4%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan5%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit5%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit5 * invoice.quan5%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv6.length && invoice.quan6.length && invoice.unit6.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+5%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv6%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan6%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit6%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit6 * invoice.quan6%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv7.length && invoice.quan7.length && invoice.unit7.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+6%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv7%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan7%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit7%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit7 * invoice.quan7%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv8.length && invoice.quan8.length && invoice.unit8.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+7%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv8%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan8%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit8%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit8 * invoice.quan8%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv9.length && invoice.quan9.length && invoice.unit9.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+8%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv9%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan9%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit9%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit9 * invoice.quan9%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv10.length && invoice.quan10.length && invoice.unit10.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+9%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv10%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan10%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit10%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit10 * invoice.quan10%></div>
                                                            </div>
                                                            <%} %>
                                                            <% if ( invoice.serv11.length && invoice.quan11.length && invoice.unit11.length !== 0) {%>
                                                            <div class="row mb-2 mb-sm-0 py-25 bgc-default-l4">
                                                                <div class="d-none d-sm-block col-1"><%=num+10%></div>
                                                                <div class="col-9 col-sm-5"><%=invoice.serv11%></div>
                                                                <div class="d-none d-sm-block col-2"><%=invoice.quan11%></div>
                                                                <div class="d-none d-sm-block col-2 text-95">KSHS <%=invoice.unit11%></div>
                                                                <div class="col-2 text-secondary-d2">KSHS <%=invoice.unit11 * invoice.quan11%></div>
                                                            </div>
                                                            <%} %>
                                                            </div>

                                                            <div class="row mt-3">
                                                                <div class="col-12 col-sm-7 text-grey-d2 text-95 mt-2 mt-lg-0">
                                                                    Payment should be done in 1 week lest we sue you
                                                                </div>

                                                                <div class="col-12 col-sm-5 text-grey text-90 order-first order-sm-last">
                                                                    <div class="row my-2">
                                                                        <div class="col-7 text-right">
                                                                            SubTotal
                                                                        </div>
                                                                        <div class="col-5">
                                                                            <% var total = Math.round(
                                                                                invoice.unit1 * invoice.quan1 +
                                                                                    invoice.unit2 * invoice.quan2 +
                                                                                    invoice.unit3 * invoice.quan3 +
                                                                                    invoice.unit4 * invoice.quan4 +
                                                                                    invoice.unit5 * invoice.quan5 +
                                                                                    invoice.unit6 * invoice.quan6 +
                                                                                    invoice.unit7 * invoice.quan7 +
                                                                                    invoice.unit8 * invoice.quan8 +
                                                                                    invoice.unit9 * invoice.quan9 +
                                                                                    invoice.unit10 * invoice.quan10 +
                                                                                    invoice.unit11 * invoice.quan11
                                                                            ) %>
                                                                            <span class="text-120 text-secondary-d1">KSHS <%= Math.round(total) %></span>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row my-2">
                                                                        <div class="col-7 text-right">
                                                                            Tax ( <%=invoice.ctax%>% )
                                                                        </div>
                                                                        <div class="col-5">
                                                                            <% var tax = total * 0.16 %>
                                                                            <span class="text-110 text-secondary-d1">KSHS <%= Math.round(tax) %></span>
                                                                        </div>
                                                                    </div>

                                                                    <div class="row my-2 align-items-center bgc-primary-l3 p-2">
                                                                        <div class="col-7 text-right">
                                                                            Total Amount
                                                                        </div>
                                                                        <div class="col-5">
                                                                            <span class="text-150 text-success-d3 opacity-2">KSHS <%= Math.round(total + tax) %></span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    <%})%>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="add_invoice">
                        <%if(invoice.length == 0){%>
                            <a href="/adminDash/addInvoice"><button class="btn_delete"><i class="fa fa-plus fa-2x"></i></button></a>
                        <%}%>
                        <div class="space_btwn"></div>
                            <%if(invoice.length > 0){%>
                                <form action="/delete/<%= invoice[0]._id %>?_method=DELETE" method="post">
                                    <button type="submit" id="btn_clear" class="clear_done">CLEAR/DONE</button>
                                </form>
                            <%}%>

                    </div>
                </div>

                    <!-- row -->


                <!-- footer -->
            </div>
            <!-- end dashboard inner -->
        </div>
    </div>

    <!-- model popup -->
    <!-- The Modal -->
    <%- include('adminPartials/modalPop')%>
    <!-- end model popup -->
</div>

<%- include('adminPartials/foot')%>